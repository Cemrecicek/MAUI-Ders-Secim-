<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="finaldeneme.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:finaldeneme.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="finaldeneme">

    <TabBar>
        <Tab Title="Home" Icon="dotnet_bot.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}"  Route="MainPage"/>
        </Tab>
    

   
        <Tab Title="Student" Icon="dotnet_bot.png">
        <ShellContent ContentTemplate="{DataTemplate local:Student}"  Route="Student"/>
        </Tab>
    

   
        <Tab Title="Course" Icon="dotnet_bot.png">
        <ShellContent ContentTemplate="{DataTemplate local:Course}"  Route="Course"/>
        </Tab>
   

   
        <Tab Title="Enroll" Icon="dotnet_bot.png">
        <ShellContent ContentTemplate="{DataTemplate local:Enroll}"  Route="Enroll"/>
        </Tab>
    </TabBar>


</Shell>
